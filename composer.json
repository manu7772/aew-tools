{
    "name": "aequation/tools",
    "type": "symfony-bundle",
    "description": "This bundle is my symfony basic tools.",
    "keywords": [ "symfony","symfony-bundle" ],
    "homepage": "https://www.aequation.fr",
    "license": "MIT",
    "authors": [
        {
            "name": "Emmanuel Dujardin / Aequation",
            "homepage": "https://github.com/manu7772"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aequation\\ToolsBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.3",
        "symfony/expression-language": "7.2.*",
        "symfony/filesystem": "7.2.*",
        "symfony/property-access": "7.2.*",
        "symfony/property-info": "7.2.*",
        "symfony/string": "7.2.*",
        "symfony/yaml": "7.2.*",
        "symfony/http-client": "7.2.*",
        "symfony/framework-bundle": "7.2.*",
        "symfony/twig-bundle": "7.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "conflict": {
        "symfony/flex": "<1.13"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": false,
            "symfony/runtime": false
        }
    },
    "minimum-stability": "stable"
}
